<header>
  <div class="container">
    <%= link_to "Community for Dogs", root_path %>
    <nav>
      <ul>
        <li><%= link_to "Home", root_path %></li>
        <li><%= link_to "Help", help_path %></li>
        <% if logged_in? %>
          <li><%= link_to "Users", '#' %></li>
          <% if current_user.dogs.any? %>
            <li><%= link_to "User&Dogs Profile", current_user %></li>
          <% else %>
            <li><%= link_to "User Profile", current_user %></li>
            <li><%= link_to 'Add first dog information!!!', dogsnew_path %></li>
          <% end %>
          <li><%= link_to "Settings", edit_user_path(current_user) %></li>
          <li><%= link_to "Log out", logout_path, method: :delete %></li>
        <% else %>
          <li><%= link_to "Log in", login_path %></li>
        <% end %>
      </ul>
    </nav>
  </div>
</header>